<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.basedao.entiti.OfficesEntiti" table="offices" schema="classicmodels">
        <id name="officeCode" column="officeCode"/>
        <property name="city" column="city"/>
        <property name="phone" column="phone"/>
        <property name="addressLine1" column="addressLine1"/>
        <property name="addressLine2" column="addressLine2"/>
        <property name="state" column="state"/>
        <property name="country" column="country"/>
        <property name="postalCode" column="postalCode"/>
        <property name="territory" column="territory"/>
    </class>
</hibernate-mapping>