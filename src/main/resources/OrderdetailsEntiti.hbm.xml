<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.basedao.OrderdetailsEntiti" table="orderdetails" schema="classicmodels">
        <composite-id mapped="true" class="ua.basedao.OrderdetailsEntitiPK">
            <key-property name="orderNumber" column="orderNumber"/>
            <key-property name="productCode" column="productCode"/>
        </composite-id>
        <property name="quantityOrdered" column="quantityOrdered"/>
        <property name="priceEach" column="priceEach"/>
        <property name="orderLineNumber" column="orderLineNumber"/>
    </class>
</hibernate-mapping>