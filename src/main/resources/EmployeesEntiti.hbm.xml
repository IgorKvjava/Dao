<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.basedao.EmployeesEntiti" table="employees" schema="classicmodels">
        <id name="employeeNumber" column="employeeNumber"/>
        <property name="lastName" column="lastName"/>
        <property name="firstName" column="firstName"/>
        <property name="extension" column="extension"/>
        <property name="email" column="email"/>
        <property name="officeCode" column="officeCode"/>
        <property name="reportsTo" column="reportsTo"/>
        <property name="jobTitle" column="jobTitle"/>
    </class>
</hibernate-mapping>